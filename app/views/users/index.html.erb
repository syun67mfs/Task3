 <div class="container">
  <div class="row">
   <%= render 'users/userinfo', {user: @user, book: @book} %>
   <div class="col-lg-4">
   <h1>Users</h1>
   <table class="table">
   <%= form_for @user do |f| %>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image_id, :fill, 20, 20, fallback: "no_image.jpg", class: "img-circle pull-left profile-thumb" %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user) %></td>
    </tr>
    <% end %>
   <% end %>
   </table>
   </div>
  </div>
 </div>